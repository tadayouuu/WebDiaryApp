@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "パスワード再設定が完了したで";
}

<div class="identity-card text-center">
    <h2 class="fw-bold mb-3">@ViewData["Title"]</h2>
    <p class="text-muted mb-4">
        パスワードを新しくしたけぇ、<br />
        下のボタンからログインしんちゃい。
    </p>
    <a asp-page="./Login" class="btn btn-dark rounded-pill w-100 py-2">ログインページへ</a>
</div>
